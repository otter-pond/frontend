{
  "name": "black-dashboard-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/react": "16.9.35",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "classnames": "2.2.6",
    "dompurify": "^2.1.1",
    "history": "4.10.1",
    "moment": "^2.25.3",
    "multiselect-react-dropdown": "^1.5.0",
    "node-sass": "^7.0.0",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "16.13.1",
    "react-big-calendar": "^0.24.6",
    "react-bootstrap-typeahead": "^5.1.2",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "16.13.1",
    "react-google-maps": "9.4.5",
    "react-icons": "^3.10.0",
    "react-loading-overlay": "^1.0.1",
    "react-notification-alert": "0.0.12",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.1",
    "react-select": "^3.1.0",
    "react-stripe-elements": "^6.1.2",
    "react-table": "^7.1.0",
    "react-table-hoc-fixed-columns": "^2.3.0",
    "reactstrap": "8.4.1",
    "universal-cookie": "^4.0.3"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=local NODE_PATH=./src react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-scss": "node-sass src/assets/scss/black-dashboard-react.scss src/assets/css/black-dashboard-react.css",
    "predeploy": "REACT_APP_STAGE=dev npm run build",
    "deploy": "aws s3 sync build/ s3://$DEVS3BUCKET",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id $DEVCLOUDFRONTID --paths '/*'",
    "predeploy:prod": "REACT_APP_STAGE=prod npm run build",
    "deploy:prod": "aws s3 sync build/ s3://$PRODS3BUCKET",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id $PRODCLOUDFRONTID --paths '/*'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
